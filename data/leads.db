CREATE TABLE IF NOT EXISTS leads_schools (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    slug TEXT,
    uid TEXT UNIQUE,
    user_name TEXT,
    linkedin_profile_url TEXT,
    linkedin_image_url TEXT,
    title TEXT,
    location TEXT,
    req_school TEXT,
    req_country TEXT,
    timestamp TEXT
);

CREATE TABLE IF NOT EXISTS leads_salesnav (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    slug TEXT,
    uid TEXT UNIQUE,
    user_name TEXT,
    linkedin_profile_url TEXT,
    linkedin_image_url TEXT,
    title TEXT,
    location TEXT,
    about TEXT,
    headline TEXT,
    skills TEXT,
    experience TEXT,
    req_school TEXT,
    req_country TEXT,
    timestamp TEXT
);

